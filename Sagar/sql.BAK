Set Safety Off



Select SLIP1.ORDERNO,SLIP1.WPUNIT, SLIP2.PNAME,slip3.pname ;
	FROM SLIP1,SLIP2,slip3;
	WHERE Alltrim(SLIP1.ORDERNO)==SLIP2.ORDERNO .and. Alltrim(SLIP1.ORDERNO)==SLIP3.ORDERNO.And. Empty(SLIP2.SENDTO).and. Empty(SLIP3.SENDTO) Into Table XTEST
